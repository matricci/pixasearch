{
  "name": "pixasearch",
  "version": "1.0.3",
  "description": "Search stock images",
  "author": "Matheus Ricci <matricci2011@hotmail.com>",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "react-start": "react-scripts start",
    "dev": "webpack --watch",
    "start": " npm run dev & electron .",
    "build": "webpack && electron-builder",
    "dist": "electron-builder",
    "dist:all": "npm-run-all -p dist:*",
    "dist:mac": "electron-builder --mac",
    "dist:windows": "electron-builder --windows",
    "dist:linux": "electron-builder --linux"
  },
  "repository": "https://github.com/matricci/pixasearch",
  "keywords": [
    "electronjs",
    "reactjs",
    "redux"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "babel": "^6.23.0",
    "css-loader": "^2.1.0",
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0",
    "react-hot-loader": "^4.12.20",
    "style-loader": "^0.23.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "axios": "^0.19.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "material-ui-flat-pagination": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "reactn": "^1.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^2.1.2",
    "minimist": ">=1.2.2"
  },
  "build": {
    "appId": "com.matheusricci.pixasearch",
    "productName": "Pixasearch",
    "mac": {
      "category": "public.app-category.developer-tools",
      "darkModeSupport": true,
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "category": "Utility",
      "icon": "icon.svg",
      "desktop": "pixasearch.desktop",
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "pacman"
      ]
    },
    "win": {
      "target": [
        "portable",
        "msi"
      ],
      "icon": "build/icon.ico"
    }
  }
}
