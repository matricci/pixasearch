{
  "name": "pixasearch",
  "version": "1.0.2",
  "description": "Search stock images",
  "author": "Matheus Ricci <matricci2011@hotmail.com>",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "react-start": "react-scripts start",
    "dev": "webpack --watch",
    "start": " npm run dev & electron .",
    "build": "webpack && electron-builder",
    "dist": "electron-builder",
    "dist:all": "npm-run-all -p dist:*",
    "dist:mac": "electron-builder --mac",
    "dist:windows": "electron-builder --windows",
    "dist:linux": "electron-builder --linux"
  },
  "repository": "https://github.com/matricci/pixasearch",
  "keywords": [
    "electronjs",
    "reactjs",
    "redux"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel": "^6.23.0",
    "css-loader": "^2.1.0",
    "electron": "^6.0.12",
    "electron-builder": "^20.44.4",
    "react-hot-loader": "^4.7.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.10.2",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "axios": "^0.19.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "fs": "0.0.1-security",
    "material-ui-flat-pagination": "^4.0.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^4.3.1",
    "reactn": "^1.0.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "remote": "^0.2.6",
    "yarn": "^1.13.0"
  },
  "build": {
    "appId": "com.matheusricci.pixasearch",
    "productName": "Pixasearch",
    "mac":{
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "category": "Utility",
      "icon": "icon.svg",
      "desktop": "pixasearch.desktop",
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "win": {
      "target": [
        "portable",
        "msi"
      ],
      "icon": "build/icon.ico"
    }
  }
}
